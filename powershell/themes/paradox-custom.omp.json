{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "prefix": ""
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "#ffff00",
          "properties": {
            "root_icon": "",
            "prefix": ""
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "prefix": ""
          },
          "style": "plain",
          "type": "session"
        },
        {
          "background": "#007ACC",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "  ",
            "style": "full"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#0000BB",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "poshgit"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "",
          "properties": {
            "prefix": "  ",
            "display_mode": "environment"
          },
          "style": "powerline",
          "type": "python"
        },
        {
          "background": "#cc0000",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "prefix": " "
          },
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "postfix": "]",
            "prefix": "["
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "properties": {
            "prefix": "",
            "text": "❯"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false
}
